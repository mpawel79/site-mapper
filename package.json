{
  "name": "site-mapper",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "node --loader ts-node/esm src/cli.ts",
    "start:headed": "node --loader ts-node/esm src/cli.ts",
    "start:headless": "HEADLESS=true node --loader ts-node/esm src/cli.ts",
    "start:slow": "SLOW_MO=2000 node --loader ts-node/esm src/cli.ts",
    "start:desktop": "node --loader ts-node/esm src/cli.ts desktop",
    "start:ipad": "node --loader ts-node/esm src/cli.ts ipad",
    "start:mobile": "node --loader ts-node/esm src/cli.ts mobile",
    "start:mobile-android": "node --loader ts-node/esm src/cli.ts mobile-android",
    "start:tablet-android": "node --loader ts-node/esm src/cli.ts tablet-android",
    "build": "tsc",
    "identify-workflows": "node --loader ts-node/esm src/identify-workflows.ts",
    "prepare-playwright": "npx playwright install"
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "cytoscape": "^3.25.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "fs-extra": "^11.3.2",
    "playwright": "^1.40.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^18.19.128",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  }
}
